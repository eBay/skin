{
    "modules": {
        "core": ["global", "utility"],
        "combo": ["core", "lightbox-dialog", "form", "progress-spinner"],
        "form": ["button", "checkbox", "field", "radio", "select", "switch", "textbox"]
    },
    "skip": ["mixins", "primitives", "tokens", "svg"],
    "skipIndex": [
        "dark-mode",
        "legacy-textbox",
        "legacy-theme",
        "rounded-off",
        "square-corner-theme"
    ],
    "dsVersions": ["6", "4"],
    "defaultDS": "6",
    "dsSkip": {
        "4": ["marketsans", "form", "core"]
    },
    "dsPathSkip": ["marketsans"],
    "overrideFile": {
        "svg": "icons.svg"
    },
    "addModules": {
        "drawer-dialog": ["icon-button"],
        "fullscreen-dialog": ["icon-button"],
        "infotip": ["icon-button"],
        "lightbox-dialog": ["icon-button"],
        "listbox-button": ["expand-button"],
        "menu-button": ["expand-button"],
        "page-notice": ["button"],
        "pagination": ["icon-button"],
        "panel-dialog": ["icon-button"],
        "section-notice": ["button"],
        "split-button": ["button", "menu-button"],
        "toast-dialog": ["icon-button"],
        "tourtip": ["icon-button"]
    }
}
